  "license": "AGPL-3.0",
  "keywords": [
    "home",
    "automation",
    "control",
    "monitor",
    "security",
    "surveillance"
  ],
  "private": true,
  "scripts": {
    "link": "npm link @theatersoft/bus @theatersoft/components",
    "build": "time -p node build.js && ls -l dist",
    "build:dist": "DIST=true npm run build",
    "build-bundle": "time -p node build.js bundle",
    "build-css": "node build.js css",
    "build-html": "node build.js html",
    "build-svg": "node build.js svg",
    "build-test-all": "node build.js test-all",
    "build-test-css": "node build.js test-css",
    "build-test-watch-reload": "node build.js test-all && node build.js test-watch & node build.js test-reload & wait",
    "build-watch": "node build.js watch",
    "build-watch-css": "node build.js watch-css",
    "publish": "node build.js publish"
  },
  "distScripts": {
    "link": "npm link"
  },
  "dependencies": {},
  "devDependencies": {
    "@theatersoft/bus": "~1",
    "@theatersoft/components": "~1",
    "babel-core": "^6.18.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-babili": "0.0.8",
    "chokidar": "^1.6.1",
    "hammerjs": "~1.0.11",
    "livereload": "^0.6.0",
    "mustache": "^2.1.3",
    "nib": "~1.0.3",
    "preact": "^6.4.0",
    "preact-redux": "^2.0.1",
    "react-redux": "^5.0.0",
    "redux": "^3.6.0",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.4.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-watch": "^3.2.2",
